{
  "presets": [
    "react",
    "es2015",
    ["env", {
      "targets": {
        "browsers": "last 2 versions",
        "node": "current"
      },
      "loose": true,
      "modules": false
    }],
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-object-rest-spread"
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "server": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  }
}
